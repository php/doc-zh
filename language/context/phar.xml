<refentry xml:id="context.phar" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" role="noversion">
 <refnamediv>
  <refname>Phar 上下文（context）选项</refname>
  <refpurpose>Phar 上下文（context）选项列表</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <para>
   <literal>phar://</literal> 封装（wrapper）的上下文（context）选项。
  </para>
 </refsect1>

 <refsect1 role="options"><!-- {{{ -->
  &reftitle.options;
  <para>
   <variablelist>
    <varlistentry xml:id="context.phar.compress">
     <term>
      <parameter>compress</parameter>
      <type>int</type>
     </term>
     <listitem>
      <para>
       <link linkend="phar.constants.compression">Phar compression constants</link> 中的一个。
      </para>
     </listitem>
    </varlistentry>
    <varlistentry xml:id="context.phar.metadata">
     <term>
      <parameter>metadata</parameter>
      <type>mixed</type>
     </term>
     <listitem>
      <para>
       Phar 元数据（metadata）。查看 <function>Phar::setMetadata</function>。
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1><!-- }}} -->
 
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><xref linkend="wrappers.phar" /></member>
    <member><link linkend="phar.using.stream">Phar stream wrapper</link></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
 
