<refentry xml:id="function.rrd-restore" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>rrd_restore</refname>
  <refpurpose>从 XML 存储中恢复 RRD 文件</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>rrd_restore</methodname>
   <methodparam><type>string</type><parameter>xml_file</parameter></methodparam>
   <methodparam><type>string</type><parameter>rrd_file</parameter></methodparam>
   <methodparam choice="opt"><type>array</type><parameter>options</parameter></methodparam>
  </methodsynopsis>
  <para>
    从 XML 存储中恢复 RRD 文件。
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>xml_file</parameter></term>
    <listitem>
     <para>
      带有原始 RRD 数据库文件转储的 XML 文件名。
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>rrd_file</parameter></term>
    <listitem>
     <para>
      恢复的 RRD 数据库文件名称。
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>options</parameter></term>
    <listitem>
     <para>
      恢复的选项数组。详情见 rrd restore 手册页。
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>
</refentry>

