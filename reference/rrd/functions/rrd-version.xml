<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="function.rrd-version">
 <refnamediv>
  <refname>rrd_version</refname>
  <refpurpose>获取底层 rrdtool 库的相关信息</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>rrd_version</methodname>
   <void/>
  </methodsynopsis>
  <simpara>
   返回底层 rrdtool 的相关信息。
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   rrdtool 版本的字符串版本号。例如：“1.4.3”。
  </simpara>
 </refsect1>

</refentry>
