<refentry xml:id="function.zend-version" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>zend_version</refname>
  <refpurpose>获取当前 Zend 引擎的版本</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>zend_version</methodname>
   <void/>
  </methodsynopsis>
  <para>
   
   获取当前运行的 Zend 引擎的版本字符串。
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   获取 Zend 引擎的版本数字的字符串。
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>zend_version</function> 例子</title>
    <programlisting role="php">
<![CDATA[
<?php
echo "Zend engine version: " . zend_version();
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
Zend engine version: 2.2.0
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>phpinfo</function></member>
    <member><function>phpcredits</function></member>
    <member><function>php_logo_guid</function></member>
    <member><function>phpversion</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

