  <refentry id="function.getcwd">
   <refnamediv>
    <refname>getcwd</refname>
    <refpurpose>取得当前工作目录</refpurpose>
   </refnamediv>
   <refsect1>
    <title>说明</title>
     <methodsynopsis>
      <type>string</type><methodname>getcwd</methodname>
      <void/>
     </methodsynopsis>
    <para>
     返回当前工作目录。
    </para>
    <para>
     参见 <function>chdir</function>。
    </para>
   </refsect1>
  </refentry>

