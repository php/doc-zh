  <refentry id="function.ociserverversion">
   <refnamediv>
    <refname>ociserverversion</refname>
    <refpurpose>返回一个包含服务器版本信息的字符串</refpurpose>
   </refnamediv>
   <refsect1>
    <title>描述</title>
     <methodsynopsis>
      <type>string</type><methodname>ociserverversion</methodname>
      <methodparam><type>int</type><parameter>conn</parameter></methodparam>
     </methodsynopsis>
    <para>
     <example>
      <title>ociserverversion</title>
      <programlisting role="php">
<![CDATA[
<?php
   $conn = OCILogon("scott", "tiger");
   echo "Server Version: " . OCIServerVersion($conn);
   OCILogOff($conn);
?>
]]>
      </programlisting>
     </example>
    </para>
   </refsect1>
  </refentry>

