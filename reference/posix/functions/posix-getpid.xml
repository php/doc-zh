<refentry xml:id="function.posix-getpid" xmlns="http://docbook.org/ns/docbook">
    <refnamediv>
        <refname>posix_getpid</refname>
        <refpurpose>返回当前进程 id</refpurpose>
    </refnamediv>

    <refsect1 role="description">
        &reftitle.description;
        <methodsynopsis>
            <type>int</type><methodname>posix_getpid</methodname>
            <void/>
        </methodsynopsis>
        <para>
            Return the process identifier of the current process.
            返回当前进程的 id
        </para>
    </refsect1>

    <refsect1 role="returnvalues">
        &reftitle.returnvalues;
        <para>
            返回进程 id 号，是整型（<type>integer</type>）。
        </para>
    </refsect1>

    <refsect1 role="examples">
        &reftitle.examples;
        <para>
            <example>
                <title><function>posix_getpid</function> 的使用例子</title>
                <programlisting role="php">
                    <![CDATA[
<?php
echo posix_getpid(); //8805
?>
]]>
                </programlisting>
            </example>
        </para>
    </refsect1>

    <refsect1 role="seealso">
        &reftitle.seealso;
        <para>
            <simplelist>
                <member><function>posix_kill</function></member>
                <member>POSIX man page GETPID(2)</member>
            </simplelist>
        </para>
    </refsect1>

</refentry>

