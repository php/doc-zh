<refentry xmlns="http://docbook.org/ns/docbook" xml:id="imagick.setimageformat">
 <refnamediv>
  <refname>Imagick::setImageFormat</refname>
  <refpurpose>为图片设置指定的格式</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>Imagick::setImageFormat</methodname>
   <methodparam><type>string</type><parameter>format</parameter></methodparam>
  </methodsynopsis>
  <para>
   为图片设置指定的格式
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>format</parameter></term>
     <listitem>
      <para>
       表示图片格式的一个字符串。支持的格式依赖于 ImageMagick 的安装。
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &imagick.return.success;
  </para>
 </refsect1>

</refentry>

