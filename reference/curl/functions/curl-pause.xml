   &curl.ch.description;
   <varlistentry>
    <term><parameter>flags</parameter></term>
    <listitem>
     <para>
      <constant>CURLPAUSE_*</constant> 常量之一。
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

