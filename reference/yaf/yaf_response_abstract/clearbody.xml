<refentry xml:id="yaf-response-abstract.clearbody" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Yaf_Response_Abstract::clearBody</refname>
  <refpurpose>丢弃所有已存在的响应 body</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>bool</type><methodname>Yaf_Response_Abstract::clearBody</methodname>
   <methodparam choice="opt"><type>string</type><parameter>key</parameter></methodparam>
  </methodsynopsis>
  <para>
   清除已存在的内容
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>key</parameter></term>
    <listitem>
     <para>
      内容 key，如果没有指定，将会清除所有内容。
      <note>
       <para>
        此参数自 2.2.0 起引入
      </para>
      </note>
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>

  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <simplelist>
   <member><methodname>Yaf_Response_Abstract::setBody</methodname></member>
   <member><methodname>Yaf_Response_Abstract::appendBody</methodname></member>
   <member><methodname>Yaf_Response_Abstract::prependBody</methodname></member>
   <member><methodname>Yaf_Response_Abstract::getBody</methodname></member>
  </simplelist>
 </refsect1>

</refentry>

