<refentry xml:id="reflectionextension.getversion" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionExtension::getVersion</refname>
  <refpurpose>获取扩展版本号</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>string</type><methodname>ReflectionExtension::getVersion</methodname>
   <void />
  </methodsynopsis>
  <para>
   获取扩展的版本号。
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   扩展的版本号。
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><methodname>ReflectionExtension::getVersion</methodname> example</title>
    <programlisting role="php">
<![CDATA[
<?php
$ext = new ReflectionExtension('mysqli');
var_dump($ext->getVersion());
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
string(3) "0.1"
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ReflectionExtension::info</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

